{
  "version": 2,
  "buildCommand": "cd packages/ui && npm run build",
  "outputDirectory": "packages/ui/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "packages/api/server.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}